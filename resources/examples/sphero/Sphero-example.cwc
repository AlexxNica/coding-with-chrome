    "blockly": "<xml xmlns=\"http://www.w3.org/1999/xhtml\">\n  <block type=\"sphero_rgb\" x=\"39\" y=\"25\">\n    <field name=\"colour\">#000000</field>\n    <next>\n      <block type=\"controls_repeat_ext\">\n        <value name=\"TIMES\">\n          <block type=\"math_number\">\n            <field name=\"NUM\">3</field>\n          </block>\n        </value>\n        <statement name=\"DO\">\n          <block type=\"sphero_backlight\">\n            <field name=\"brightness\">254</field>\n            <next>\n              <block type=\"sphero_backlight\">\n                <field name=\"brightness\">0</field>\n              </block>\n            </next>\n          </block>\n        </statement>\n        <next>\n          <block type=\"controls_repeat_ext\">\n            <value name=\"TIMES\">\n              <block type=\"math_number\">\n                <field name=\"NUM\">3</field>\n              </block>\n            </value>\n            <statement name=\"DO\">\n              <block type=\"sphero_rgb\">\n                <field name=\"colour\">#ff0000</field>\n                <next>\n                  <block type=\"sphero_rgb\">\n                    <field name=\"colour\">#000000</field>\n                  </block>\n                </next>\n              </block>\n            </statement>\n            <next>\n              <block type=\"controls_repeat_ext\">\n                <value name=\"TIMES\">\n                  <block type=\"math_number\">\n                    <field name=\"NUM\">3</field>\n                  </block>\n                </value>\n                <statement name=\"DO\">\n                  <block type=\"sphero_rgb\">\n                    <field name=\"colour\">#33ff33</field>\n                    <next>\n                      <block type=\"sphero_rgb\">\n                        <field name=\"colour\">#000000</field>\n                      </block>\n                    </next>\n                  </block>\n                </statement>\n                <next>\n                  <block type=\"sphero_rgb\">\n                    <field name=\"colour\">#ffff00</field>\n                    <next>\n                      <block type=\"controls_repeat_ext\">\n                        <value name=\"TIMES\">\n                          <block type=\"math_number\">\n                            <field name=\"NUM\">4</field>\n                          </block>\n                        </value>\n                        <statement name=\"DO\">\n                          <block type=\"sphero_roll_time\">\n                            <field name=\"heading\">0</field>\n                            <value name=\"time\">\n                              <block type=\"math_number\">\n                                <field name=\"NUM\">2</field>\n                              </block>\n                            </value>\n                            <value name=\"speed\">\n                              <block type=\"math_number\">\n                                <field name=\"NUM\">40</field>\n                              </block>\n                            </value>\n                            <next>\n                              <block type=\"sphero_roll_time\">\n                                <field name=\"heading\">90</field>\n                                <value name=\"time\">\n                                  <block type=\"math_number\">\n                                    <field name=\"NUM\">2</field>\n                                  </block>\n                                </value>\n                                <value name=\"speed\">\n                                  <block type=\"math_number\">\n                                    <field name=\"NUM\">40</field>\n                                  </block>\n                                </value>\n                                <next>\n                                  <block type=\"sphero_roll_time\">\n                                    <field name=\"heading\"