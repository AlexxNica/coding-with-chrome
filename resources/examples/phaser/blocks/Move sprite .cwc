    "blockly": "<xml xmlns=\"http://www.w3.org/1999/xhtml\">\n  <block type=\"phaser_game\" x=\"1204\" y=\"-1096\">\n    <field name=\"name\">Spaceship</field>\n    <field name=\"width\">800</field>\n    <field name=\"height\">600</field>\n    <next>\n      <block type=\"phaser_preload\">\n        <statement name=\"CODE\">\n          <block type=\"phaser_load_image\">\n            <field name=\"name\">ship_sprite</field>\n            <value name=\"image\">\n              <block type=\"general_file_library_image\">\n                <value name=\"FILENAME\">\n                  <shadow type=\"text\">\n                    <field name=\"TEXT\">sphero.png</field>\n                  </shadow>\n                </value>\n              </block>\n            </value>\n          </block>\n        </statement>\n        <next>\n          <block type=\"phaser_create\">\n            <statement name=\"CODE\">\n              <block type=\"phaser_stage_background_color\">\n                <field name=\"color\">#ccffff</field>\n                <next>\n                  <block type=\"variables_set\">\n                    <field name=\"VAR\">title</field>\n                    <value name=\"VALUE\">\n                      <block type=\"phaser_add_text\">\n                        <field name=\"x\">10</field>\n                        <field name=\"y\">10</field>\n                        <field name=\"text\">My first game</field>\n                      </block>\n                    </value>\n                    <next>\n                      <block type=\"variables_set\">\n                        <field name=\"VAR\">sprite</field>\n                        <value name=\"VALUE\">\n                          <block type=\"phaser_add_sprite\">\n                            <field name=\"x\">150</field>\n                            <field name=\"y\">150</field>\n                            <field name=\"sprite\">ship_sprite</field>\n                          </block>\n                        </value>\n                        <next>\n                          <block type=\"variables_set\">\n                            <field name=\"VAR\">cursors</field>\n                            <value name=\"VALUE\">\n                              <block type=\"phaser_input_keyboard_create_cursor_keys\"></block>\n                            </value>\n                            <next>\n                              <block type=\"phaser_sprite_adjust\">\n                                <field name=\"property\">anchor.set</field>\n                                <value name=\"sprite\">\n                                  <block type=\"variables_get\">\n                                    <field name=\"VAR\">sprite</field>\n                                  </block>\n                                </value>\n                                <value name=\"value\">\n                                  <block type=\"math_number\">\n                                    <field name=\"NUM\">0.5</field>\n                                  </block>\n                                </value>\n                                <next>\n                                  <block type=\"phaser_physics_arcade_enable\">\n                                    <value name=\"object\">\n                                      <block type=\"variables_get\">\n                                        <field name=\"VAR\">sprite</field>\n                                      </block>\n                                    </value>\n                                  </block>\n                                </next>\n                              </block>\n                            </next>\n                          </block>\n                        </next>\n                      </block>\n                    </next>\n                  </block>\n                </next>\n              </block>\n            </statement>\n            <next>\n              <block type=\"phaser_update\">\n                <statement name=\"