field>\n                        <value name=\"sprite\">\n                          <block type=\"variables_get\">\n                            <field name=\"VAR\">sprite</field>\n                          </block>\n                        </value>\n                        <value name=\"value\">\n                          <block type=\"math_number\">\n                            <field name=\"NUM\">-10</field>\n                          </block>\n                        </value>\n                      </block>\n                    </statement>\n                    <value name=\"IF2\">\n                      <block type=\"phaser_input_keyboard_is_pressed\">\n                        <field name=\"DIRECTION\">left.isDown</field>\n                        <value name=\"cursors\">\n                          <block type=\"variables_get\">\n                            <field name=\"VAR\">cursors</field>\n                          </block>\n                        </value>\n                      </block>\n                    </value>\n                    <statement name=\"DO2\">\n                      <block type=\"phaser_sprite_adjust\">\n                        <field name=\"property\">body.angularVelocity</field>\n                        <value name=\"sprite\">\n                          <block type=\"variables_get\">\n                            <field name=\"VAR\">sprite</field>\n                          </block>\n                        </value>\n                        <value name=\"value\">\n                          <block type=\"math_number\">\n                            <field name=\"NUM\">-200</field>\n                          </block>\n                        </value>\n                      </block>\n                    </statement>\n                    <value name=\"IF3\">\n                      <block type=\"phaser_input_keyboard_is_pressed\">\n                        <field name=\"DIRECTION\">right.isDown</field>\n                        <value name=\"cursors\">\n                          <block type=\"variables_get\">\n                            <field name=\"VAR\">cursors</field>\n                          </block>\n                        </value>\n                      </block>\n                    </value>\n                    <statement name=\"DO3\">\n                      <block type=\"phaser_sprite_adjust\">\n                        <field name=\"property\">body.angularVelocity</field>\n                        <value name=\"sprite\">\n                          <block type=\"variables_get\">\n                            <field name=\"VAR\">sprite</field>\n                          </block>\n                        </value>\n                        <value name=\"value\">\n                          <block type=\"math_number\">\n                            <field name=\"NUM\">200</field>\n                          </block>\n                        </value>\n                      </block>\n                    </statement>\n                  </block>\n                </statement>\n              </block>\n            </next>\n          </block>\n        </next>\n      </block>\n    </next>\n  </block>\n</xml>",
    "javascript": "var sprite, cursors, title;\n\n\nvar game = new Phaser.Game(  800, 600, Phaser.CANVAS, 'Spaceship' , {\n  preload: typeof preload !== 'undefined' ? preload : function() {},\n  create: typeof create !== 'undefined' ? create : null,\n  update: typeof update !== 'undefined' ? update : null,\n  render: typeof render !== 'undefined' ? render : null\n});\nfunction preload() {\n  game.load.image('ship_sprite', '  {{ file:sphero.png }}');\n\n};\nfunction create() {\n  game.stage.backgroundColor = '#ccffff';\n  title = (game.add.text(10, 10, 'My first game'));\n  sprite = (game.add.sprite(150, 150, 'ship_sprite'));\n  cursors = (this.input.keyboard.createCursorKeys());\n  sprite.anchor.set(0.5);\n  game.physics.arcade.enable(sprite);\n\n};\nfunction update() {\n  if (cursors.up.isDown) {\n    sprite.body.acceleration.set(10);\n  } else if (cursors.down.isDown) {\n    sprite.body.acceleration.set(-10);\n  } else if (cursors.left.isDown) {\n    sprite.body.angularVelocity = -200;\n  } else if (cursors.right.isDown) {\n    sprite.body.angularVelocity = 200;\n  }\n\n};\n"
