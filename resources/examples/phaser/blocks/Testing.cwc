    "blockly": "<xml xmlns=\"http://www.w3.org/1999/xhtml\">\n  <block type=\"phaser_game\" x=\"1314\" y=\"-1142\">\n    <field name=\"name\">Spaceship</field>\n    <field name=\"width\">800</field>\n    <field name=\"height\">600</field>\n    <next>\n      <block type=\"phaser_preload\">\n        <statement name=\"CODE\">\n          <block type=\"phaser_load_image\">\n            <field name=\"name\">ship_sprite</field>\n            <value name=\"image\">\n              <block type=\"general_file_library_image\">\n                <value name=\"filename\">\n                  <shadow type=\"text\">\n                    <field name=\"TEXT\">space-ship.png</field>\n                  </shadow>\n                </value>\n              </block>\n            </value>\n          </block>\n        </statement>\n        <next>\n          <block type=\"phaser_create\">\n            <statement name=\"CODE\">\n              <block type=\"phaser_stage_background_color\">\n                <field name=\"color\">#ccffff</field>\n                <next>\n                  <block type=\"variables_set\">\n                    <field name=\"VAR\">title</field>\n                    <value name=\"VALUE\">\n                      <block type=\"phaser_add_text\">\n                        <field name=\"text\">Use your cursor keys to move the sprite.</field>\n                        <field name=\"x\">10</field>\n                        <field name=\"y\">10</field>\n                      </block>\n                    </value>\n                    <next>\n                      <block type=\"variables_set\">\n                        <field name=\"VAR\">sprite</field>\n                        <value name=\"VALUE\">\n                          <block type=\"phaser_add_sprite\">\n                            <field name=\"sprite\">ship_sprite</field>\n                            <field name=\"x\">150</field>\n                            <field name=\"y\">150</field>\n                          </block>\n