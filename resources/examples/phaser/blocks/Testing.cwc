/next>\n                      </block>\n                    </statement>\n                    <next>\n                      <block type=\"phaser_world_wrap\">\n                        <value name=\"sprite\">\n                          <block type=\"variables_get\">\n                            <field name=\"VAR\">sprite</field>\n                          </block>\n                        </value>\n                        <value name=\"value\">\n                          <block type=\"math_number\">\n                            <field name=\"NUM\">0</field>\n                          </block>\n                        </value>\n                      </block>\n                    </next>\n                  </block>\n                </statement>\n              </block>\n            </next>\n          </block>\n        </next>\n      </block>\n    </next>\n  </block>\n</xml>",
    "javascript": "var sprite, cursors, spaceKey, title;\n\n\nvar game = new Phaser.Game(  800, 600, Phaser.CANVAS, 'Spaceship' , {\n  preload: typeof preload !== 'undefined' ? preload : function() {},\n  create: typeof create !== 'undefined' ? create : null,\n  update: typeof update !== 'undefined' ? update : null,\n  render: typeof render !== 'undefined' ? render : null\n});\nfunction preload() {\n  game.load.image('ship_sprite', '{{ file:space-ship.png }}');\n};\nfunction create() {\n  game.stage.backgroundColor = '#ccffff';\n  title = (game.add.text(10, 10, 'Use your cursor keys to move the sprite.'));\n  sprite = (game.add.sprite(150, 150, 'ship_sprite'));\n  cursors = (this.input.keyboard.createCursorKeys());\n  spaceKey = (this.input.keyboard.addKey(Phaser.KeyCode.SPACEBAR));\n  sprite.anchor.set(0.5);\n};\nfunction update() {\n  if (cursors.up.isDown) {\n    sprite.angle = 0;\n    sprite.y -= 2;\n  } else if (cursors.down.isDown) {\n    sprite.angle = 180;\n    sprite.y += 2;\n  } else if (cursors.left.isDown) {\n    sprite.angle = 270;\n    sprite.x -= 2;\n  } else if (cursors.right.isDown) {\n    sprite.angle = 90;\n    sprite.x += 2;\n  }\n  game.world.wrap(sprite, 0);\n};\n"
