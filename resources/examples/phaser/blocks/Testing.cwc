          </block>\n            </statement>\n            <next>\n              <block type=\"phaser_update\">\n                <statement name=\"CODE\">\n                  <block type=\"controls_if\">\n                    <mutation elseif=\"3\"></mutation>\n                    <value name=\"IF0\">\n                      <block type=\"phaser_input_keyboard_is_pressed\">\n                        <field name=\"direction\">up.isDown</field>\n                        <value name=\"cursors\">\n                          <block type=\"variables_get\">\n                            <field name=\"VAR\">cursors</field>\n                          </block>\n                        </value>\n                      </block>\n                    </value>\n                    <statement name=\"DO0\">\n                      <block type=\"phaser_sprite_adjust\">\n                        <field name=\"property\">angle</field>\n                        <value name=\"sprite\">\n                          <block type=\"variables_get\">\n                            <field name=\"VAR\">sprite</field>\n                          </block>\n                        </value>\n                        <value name=\"value\">\n                          <block type=\"math_number\">\n                            <field name=\"NUM\">0</field>\n                          </block>\n                        </value>\n                        <next>\n                          <block type=\"phaser_sprite_adjust\">\n                            <field name=\"property\">moveUp</field>\n                            <value name=\"sprite\">\n                              <block type=\"variables_get\">\n                                <field name=\"VAR\">sprite</field>\n                              </block>\n                            </value>\n                            <value name=\"value\">\n                              <block type=\"math_number\">\n                                <field name=\"NUM\">2</field>\n     